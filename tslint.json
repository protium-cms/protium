{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": ["node_modules/**/*"]
  },
  "rules": {
    "no-console": false,
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "space-before-function-paren": [true, "always"]
  },
  "rulesDirectory": []
}
