{
  "name": "@protium/core",
  "version": "0.0.1",
  "author": "Jon Jaques <jaquers@gmail.com>",
  "license": "MIT",
  "main": "./lib/index.js",
  "source": "./src/index.ts",
  "engines": {
    "node": ">=10",
    "yarn": ">=1.7"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "config": "^3.0.1",
    "json2yaml": "^1.1.0",
    "lodash": "^4.17.11",
    "logform": "^2.1.0",
    "pretty-error": "^2.1.1",
    "resolve-pkg": "^1.0.0",
    "winston": "^3.1.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "gen-env-config": "NODE_CONFIG_DIR=base cev --prefix PROTIUM > _tmp_env",
    "yaml-env-config": "json2yaml '_tmp_env' >> base/custom-environment-variables.yaml && rm _tmp_env",
    "write-env-config": "run-s gen-env-config write-env-config-notice yaml-env-config",
    "write-env-config-notice": "echo '# AUTOGENERATED, run \"yarn generate-env-config\" if you make schema changes' > base/custom-environment-variables.yaml",
    "generate-env-config": "yarn write-env-config"
  },
  "_moduleAliases": {
    "react-native": "node_modules/react-native-web"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/lodash": "^4.14.119",
    "config-cev-generator": "^0.1.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.2"
  }
}
